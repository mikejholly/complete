{
  "manifest_version": 2,
  "name": "Auto-complete",
  "short_name": "Auto-complete",
  "description": "Auto-complete for the desktop web.",
  "version": "0.1.2",
  "icons": {
    "540": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "words.js",
        "main.js"
      ]
    }
  ]
}
